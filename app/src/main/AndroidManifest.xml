<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.fantasybasketballproject">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FantasyBasketballProject"
        tools:targetApi="31">
        <activity
            android:name=".Leaderboard"
            android:exported="false" />
        <activity
            android:name=".ThirdRound"
            android:exported="false" />
        <activity
            android:name=".SecondRound"
            android:exported="false" />
        <activity
            android:name=".FirstRound"
            android:exported="false" />
        <activity
            android:name=".HeadToHead"
            android:exported="false" />
        <activity
            android:name=".ReservePlayers"
            android:exported="false" />
        <activity
            android:name=".PlayerStatsClass"
            android:exported="false" />
        <activity
            android:name=".ActivePlayerHome"
            android:exported="false" />
        <activity
            android:name=".BreakoutRoom"
            android:exported="false" />
        <activity
            android:name=".Roster"
            android:exported="false" />
        <activity
            android:name=".ReadyToDraft"
            android:exported="false" />
        <activity
            android:name=".SignInScreen"
            android:exported="true" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>